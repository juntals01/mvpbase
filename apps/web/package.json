{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env -- sh -c 'PORT=${WEB_PORT:-3000} next dev --turbopack'",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "start:local": "dotenv -e ../../.env -- sh -c 'PORT=${WEB_PORT:-3000} next start -p $PORT'",
